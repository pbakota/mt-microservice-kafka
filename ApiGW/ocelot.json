        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/gateway/orders",
      "UpstreamHttpMethod": [
        "Post", "Get"
      ]
    },
    {
      "DownstreamPathTemplate": "/payments",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
